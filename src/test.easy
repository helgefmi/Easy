def test3 do
    printf("before if!\n")
    if strcmp("a" "a") then
        printf("1\n")
    else
        if 1 then
            printf("2\n")
        else
            printf("3\n")
        end
    end
    printf("after if\n")
end

def test2 do
    write(0 "Lorem ipsum dolor sit amet" 5)
